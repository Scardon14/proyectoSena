<?php
class ClienteModel
{
    private $idCliente;
    private $nombres;
    private $apellidos;
    private $numeroCelular;
    private $correoElectronico;
    private $fechaCreacion;
    private $estado;
    private $idUsuario;

    public function __construct(
        $idCliente = null,
        $nombres = null,
        $apellidos = null,
        $numeroCelular = null,
        $correoElectronico = null,
        $fechaCreacion = null,
        $estado = null,
        $idUsuario = null
    ) {
        $this->idCliente = $idCliente;
        $this->nombres = $nombres;
        $this->apellidos = $apellidos;
        $this->numeroCelular = $numeroCelular;
        $this->correoElectronico = $correoElectronico;
        $this->fechaCreacion = $fechaCreacion;
        $this->estado = $estado;
        $this->idUsuario = $idUsuario;
    }

    // Getter y Setter para idCliente
    public function getIdCliente()
    {
        return $this->idCliente;
    }

    public function setIdCliente($idCliente)
    {
        $this->idCliente = $idCliente;
    }

    // Getter y Setter para nombres
    public function getNombres()
    {
        return $this->nombres;
    }

    public function setNombres($nombres)
    {
        $this->nombres = $nombres;
    }

    // Getter y Setter para apellidos
    public function getApellidos()
    {
        return $this->apellidos;
    }

    public function setApellidos($apellidos)
    {
        $this->apellidos = $apellidos;
    }

    // Getter y Setter para numeroCelular
    public function getNumeroCelular()
    {
        return $this->numeroCelular;
    }

    public function setNumeroCelular($numeroCelular)
    {
        $this->numeroCelular = $numeroCelular;
    }

    // Getter y Setter para correoElectronico
    public function getCorreoElectronico()
    {
        return $this->correoElectronico;
    }

    public function setCorreoElectronico($correoElectronico)
    {
        $this->correoElectronico = $correoElectronico;
    }

    // Getter y Setter para fechaCreacion
    public function getFechaCreacion()
    {
        return $this->fechaCreacion;
    }

    public function setFechaCreacion($fechaCreacion)
    {
        $this->fechaCreacion = $fechaCreacion;
    }

    // Getter y Setter para estado
    public function getEstado()
    {
        return $this->estado;
    }

    public function setEstado($estado)
    {
        $this->estado = $estado;
    }

    // Getter y Setter para idUsuario
    public function getIdUsuario()
    {
        return $this->idUsuario;
    }

    public function setIdUsuario($idUsuario)
    {
        $this->idUsuario = $idUsuario;
    }
}
?>